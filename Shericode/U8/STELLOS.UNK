uses "npc";
uses "free";
uses "snddefs";
uses "flags";


npcclass Stellos inherits Npc
{
	process use();
	process look();
	process schedule(currentTime:Long);
	process enterFastArea();
	process leaveFastArea();
	process cast(ref:Referent);
	process AvatarStoleSomething(ref:Referent);

	process atMonastery(); 
	process atHome(); 
	process randomBarkAtHome(); 
	process randomBarkAtMonastery(); 

}



process Stellos::look()
{
	if Stellos.isDead()
	{
		bark("the body of Stellos \f la despouille de Stellos \g Stellos' Leiche");
		return;
	}

	if stellosName and stellosJob 
	{
		bark("Stellos the monk \f Stellos le moine \g Der Mînch Stellos");
		return;
	}
	else if stellosName 
	{
		bark("Stellos");
		return;
	}
	else if stellosJob 
	{
		bark("monk \f moine \g Mînch");
		return;
	}
	else 
	{
		bark("man \f homme \g Mann");
		return;
	}
}




process Stellos::schedule(currentTime:Long)
{

	point : WorldPoint;
	map : MapNum = MapArgentrockIsle;
	sched:Number;

	if Stellos.isDead() {
		return;
	}

	// If time to go to work, head for the Monastery
	if (getPeriod() = lastebb) or (getPeriod() = eventide) 
	{
		point.x = 18623;
		point.y = 19199;
		point.z = 56;
		sched = at_monastery;
	}


	// if it's nap time, head for home.
	if (getPeriod() <= threemoons) 
	{
		point.x = 21790;
		point.y = 19818;
		point.z = 56;
		sched = at_home;
	}

	if canSafeTeleport(point,map)
	{
		stellosSchedule = sched;
		safeTeleport(point,map);
	}

}




process Stellos::enterFastArea()
{

	if Stellos.isDead()
	{
		return;
	}

	resetRef(referent,PT_PATHFIND);
	resetRef(referent,PT_ACTIVITY);
	resetRef(referent,PT_ANIM);
	resetRef(referent,PT_BARK);
	
	if (stellosSchedule = at_home) 
	{
		spawn atHome() -> return;
	}
	
	else if (stellosSchedule = at_monastery) 
	{
		spawn atMonastery() -> return;
	}
}



process Stellos::leaveFastArea()
{
	stellosJustHealed = false;

	killProcess(referent,PT_ACTIVITY);
	killProcess(referent,PT_PATHFIND);
	killProcess(referent,PT_ANIM);
	killProcess(referent,PT_BARK);

	if isDead()
	{
		teleport(urandom(10000),urandom(10000),0,0);
	}
}




process Stellos::cast(ref:Referent)
{
	count:Number;
	count = 10;

	// TEST TO MAKE SURE THAT THE NPC HAS A TARGET
	// -------------------------------------------
	while (not getTarget()) and (count) {
		wait(1);
		count--;
	}

	ref = getTarget();


	if ref = Avatar:Referent
	{
		resetRef(referent,PT_ACTIVITY);
		resetRef(referent,PT_PATHFIND);
		resetRef(referent,PT_ANIM);
		resetRef(referent,PT_BARK);

		bark("Stratos! Preserve me!
					\f Stratos! Espargne-moy!
					\g Stratos! Rette mich!");

		spawn safePlaySFX(MAGIC3,200) -> return;
		realDoAnim(AS_ENTER_CAST,getDir());
		realDoAnim(AS_CAST1,getDir());
		toggleStatus(INVISIBLE);
		setFrame(getFrame());
		spawn safePlaySFX( WIND4,100) -> return;
		realDoAnim(AS_EXIT_CAST,getDir());
		clrImmortal();
		setDead();
		teleport(urandom(10000),urandom(10000),0,0);
	}
}




process Stellos::AvatarStoleSomething(ref:Referent)
{
	item:Item;
	item.referent = ref;

	resetRef(referent,PT_ACTIVITY);
	resetRef(referent,PT_PATHFIND);
	resetRef(referent,PT_ANIM);
	resetRef(referent,PT_BARK);

	turnToFace(getDirToItem(Avatar:Referent),false,10);
	bark("What are you doing there...?
		\f Eh! Qu'est-ce que tu trafiques, lÖ-bas...?
		\g Was macht Ihr denn da...?");
	wait(120);
	enterFastArea();
	return;
}





process Stellos::atMonastery()
{

	[[PT_ACTIVITY, referent]];

	startup 
	{
		pathEgg:Item;
		counter,counter2:Number;
		foreach	pathEgg where (type = TypePathMarker) and (quality = 4) within 10 cells of Stellos
		{
			counter2 = pathEgg.getFrame();
		}
	}

	process
	{
		for counter = counter2 to 31 
		{
			foreach	pathEgg where (type = TypePathMarker) and (frame = counter)
			and (quality = 4) within 50 cells of Stellos 
			{
				if pathfind(pathEgg.referent,10)
				{
					if not somebodyTalking
					{
						randomBarkAtMonastery();
					}
					else
					{
						doAnim(AS_FIDGET,getDir(),NO_ANIM_LIMIT,FALSE);
						wait(30);
						doAnim(AS_FIDGET,getDir(),NO_ANIM_LIMIT,FALSE);
					}
				}
				else
				{
					wait(500);
					doAnim(AS_FIDGET,getDir(),NO_ANIM_LIMIT,FALSE);
				}
			}
		}

	counter2 = 0;
	}
}


