process Stellos::atHome()
{

	[[PT_ACTIVITY, referent]];

	startup 
	{
		pathEgg:Item;
		counter, counter2 :Number;

		if getLastAnimSet() = AS_SIT_DOWN 
		{
			doAnim(AS_SIT_UP,getDir(),NO_ANIM_LIMIT,FALSE);
		}
		foreach	pathEgg where (type = TypePathMarker) and (quality = 3) within 10 cells of Stellos
		{
			counter2 = pathEgg.getFrame();
		}
	}

	process
	{
		for counter = counter2 to 16
		{
			foreach	pathEgg where (type = TypePathMarker) and (frame = counter)
				and (quality = 3) within 25 cells of Stellos
				{
				pathfind(pathEgg.referent,10);
				if counter = 0
				{
					turnToFace(2,0,10);
					bark("I must meditate! \f Il faut que je mesdite!
						\g Ich mu· meditieren!");
					doAnim(AS_SIT_DOWN,getDir(),NO_ANIM_LIMIT,FALSE);
					wait(2000);
					doAnim(AS_SIT_UP,getDir(),NO_ANIM_LIMIT,FALSE);
					bark("I feel refreshed now! \f Je me sens bien mieux, maintenant!
						\g Jetzt fÅhle ich mich erfrischt!");
				}
				else
				{
					if not somebodyTalking
					{
						doAnim(AS_STAND,getDir(),NO_ANIM_LIMIT,FALSE);
						randomBarkAtMonastery();
					}
					else
					{
						doAnim(AS_FIDGET,getDir(),NO_ANIM_LIMIT,FALSE);
					}
				}
				wait(60 + urandom(100));
			}
		}
	counter2 = 0;
	}
}


