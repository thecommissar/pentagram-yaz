uses "flags";
uses "free";
uses "npc";


npcclass Bentic inherits Npc 
{
	process use();
	process look();
	process schedule(currentTime:Long);
	process enterFastArea();
	process leaveFastArea();
	process cast(ref:Referent);
	process AvatarStoleSomething(ref:Referent);

	process readBook();
	process randomWisdom();
}


process Bentic::look() 
{

	if isDead()
	{
		bark("Bentic the dead librarian
			\f Bentic le biblioth‚caire, mort.
			\g Bentic, der tote Bibliothekar");
		return;
	}

	if benticName and benticJob 
	{
		bark("Bentic the Librarian
			 \f Bentic le biblioth‚caire.
			 \g Bentic, der Bibliothekar");
		return;
	}

	else if benticName 
	{
		bark("Bentic");
	}

	else if benticJob 
	{
		bark("librarian \f biblioth‚caire \g Bibliothekar");
	}

	else
	{
		bark("man \f homme \g Mann");
	}
}


process Bentic::schedule(currentTime:Long) 
{

	if isDead() 
	{
		return;
	}


	point : WorldPoint;
	map : MapNum = MapEastTenebrae;

	// In the Library...

	if (getPeriod() = threemoons) or (getPeriod() = daytide) 
	{
		point.x = 13407;
		point.y = 2623;
		point.z = 48;
	}

	// Other side of Library.

	if (getPeriod() = firstebb) or (getPeriod() = lastebb) 
	{
		point.x = 14431;
		point.y = 2015;
		point.z = 48;
	}


	// time for a nap.

	if (getPeriod() = eventide) or (getPeriod() = bloodwatch) 
	{
		point.x = 15199;
		point.y = 2047;
		point.z = 96;
	}

	safeTeleport(point,map); 

}




process Bentic::enterFastArea() 
{
	if isDead() 
	{
		return;
	}

	killProcess(referent,PT_ACTIVITY);
	spawn readBook() -> return;
}




process Bentic::leaveFastArea()
{

	killProcess(referent,PT_ACTIVITY);

	if isDead() 
	{
		teleport(urandom(1000),urandom(1000),0,0);
	}
}



process Bentic::AvatarStoleSomething(ref:Referent)
{

	item:Item;
	item.referent = ref;
	spawn realDoAnim(AS_CLIMB_2,getDir()) -> return;

	if ((item.getType() = TypeBook) or (item.getType() = TypeGrimoire)) 
	{
		resetRef(referent,PT_BARK);
		resetRef(referent,PT_ACTIVITY);

		bark("Please return that within two weeks!
			\f Aies l'amabilit‚ de le rapporter dans deux semaines!
			\g Bitte bringt das Buch sp„testens in zwei Wochen zurck!");
		enterFastArea();
		return;
	}
}



process Bentic::cast(ref:Referent)
{
	count:Number;
	count = 10;

	// TEST TO MAKE SURE THAT THE NPC HAS A TARGET
	// -------------------------------------------
	while (not getTarget()) and (count) {
		wait(1);
		count--;
	}

	ref = getTarget();


	if ref = Avatar:Referent
	{
		spawn callTheGuards(NULL_REF) -> return;

		if (urandom(100) > 50)
		{

			bark("I am attacked! Help!
					\f A l'aide! On m'attaque!
					\g Hilfe! Ich werde angegriffen!");
		}
		else
		{
			bark("Help! Sorcerer!
						\f Sorcier! A l'aide!
						\g Hilfe! Hexer!");
		}


	}
}




