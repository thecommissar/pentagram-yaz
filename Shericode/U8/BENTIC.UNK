process Bentic::readBook()
{
	[[PT_ACTIVITY, referent]];

	startup
		{
		counter:Number = 0;
		}
	process
		{
		realDoAnim(AS_CLIMB_2,2);
		wait(60 + urandom(100));
		randomWisdom();
		wait(60 + urandom(100));
		}
}


process Bentic::randomWisdom()
{
	[[PT_ACTIVITY, referent]];

	ran:Number = urandom(10);

	if ran = 1 
	{
		realDoAnim(AS_FIDGET,2);
		bark("I never knew that!
			\f Mais j'ignorois cela!
			\g Das ist mir neu!");
		return;
	}

	if ran = 2 
	{
		bark("Ah, yes...
			\f Ah, oui...
			\g Ach, ja...?");
		realDoAnim(AS_FIDGET,2);
		return;
	}

	if ran = 3 
	{
		bark("Fascinating!
			\f Fascinant!
			\g Faszinierend!");
		return;
	}

	if ran = 4 
	{
		bark("Very interesting.
			\f TrŠs int‚ressant.
			\g Sehr interessant.");
		realDoAnim(AS_FIDGET,2);
		return;
			}

	if ran = 5 
	{
		bark("Such a good book.
			\f Un excellent manuscript.
			\g Ein ausgezeichnetes Buch.");
		return;
	}

	if ran = 6 
	{
		bark("hummm... \f Hmmmm.... \g Hmmmm...");
		realDoAnim(AS_FIDGET,2);
		return;
	}


	if ran = 7
	{
		bark("I must make a note of this.
			\f Il faut que je m'en souvienne.
			\g Das muá ich mir merken.");
		realDoAnim(AS_FIDGET,2);
		return;
	}

	if ran = 8
	{
		bark("I wonder if Devon knows this?
			\f Je me demande si Devon le sait.
			\g Ich frage mich, ob Devon das weiá.");
			wait(100);
		return;
	}

	else
	{
		realDoAnim(AS_FIDGET,2);
		bark("I will have to read more on this.
			\f Il faut que j'approfondisse ce sujet.
			\g Damit muá ich mich unbedingt noch n„her befassen.");
		return;
	}
