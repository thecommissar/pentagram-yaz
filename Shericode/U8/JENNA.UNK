				bark("T'as besoin de quelque chose?");
				}
				#if German
				{
				bark("Braucht Ihr etwas?");
				}
				return;
			}
			realDoAnim(AS_FIDGET,getDir());
		}
		if ran = 3 
		{
			turnToFace(getDirToItem(Avatar:Referent),false,10);
			if not orlokMet
			{
				bark("If you want something, take a seat!
