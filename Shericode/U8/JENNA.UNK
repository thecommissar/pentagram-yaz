uses "flags";
uses "free";
uses "npc";

npcclass Jenna inherits Npc
{
	process use();
	process look();
	process enterFastArea();
	process leaveFastArea();
	process cast(ref:Referent);

	process closeNSDoor();
	process closeEWDoor();
	process tendBar();
	process randomBarkAtBar();
	routine getDrink():String;
	process buyDrink();
	process giveDrink();
	routine getFood():String;
	process buyFood();
	process giveFood();
}



process Jenna::look()
{

	if isDead() 
	{
		if jennaName
		{

			bark("the corpse of Jenna \f despouille de Jenna \g Jennas Leiche");
		}

		else 
		{
			bark("dead woman \f femme morte \g Tote Frau");
		}
		return;
	}

	if jennaName and jennaJob 
	{
		bark("Jenna the barmaid \f Jenna la serveuse \g Die Kellnerin Jenna");
		return;
	}

	if jennaName
	{
		bark("Jenna");
		return;
	}

	if jennaJob 
	{
		bark("barmaid \f serveuse \g Kellnerin");
		return;
	}

	else 
	{
		bark("woman \f femme \g Frau");
	}
}




process Jenna::enterFastArea()
{
	resetRef(referent,PT_ACTIVITY);
	resetRef(referent,PT_PATHFIND);
	resetRef(referent,PT_ANIM);

	spawn tendBar() -> return;

}




process Jenna::leaveFastArea()
{

	killProcess(referent,PT_ACTIVITY);
	killProcess(referent,PT_PATHFIND);
	killProcess(referent,PT_ANIM);
	killProcess(referent,PT_BARK);

	if sendJennaToLunch
	{
		setDead();
	}


	if isDead()
	{
		teleport(urandom(10000),urandom(10000),0,0);
	}
}





process Jenna::cast(ref:Referent)
{

	count:Number;
	count = 10;

	// TEST TO MAKE SURE THAT THE NPC HAS A TARGET
	// -------------------------------------------
	while (not getTarget()) and (count) {
		wait(1);
		count--;
	}

	ref = getTarget();



	if ref = Avatar:Referent
	{
		spawn callTheGuards(NULL_REF) -> return;

		if not isDead()
		{
			if (urandom(100) > 50)
			{
	 
				bark("Help me, Beren!
					\f Aide-moy, Beren!
					\g Helft mir, Beren!");
			}
			else
			{
				bark("Town Sorcerer!
						\f A moy, Sorcier du Bourg!
						\g Stadthexer!");
			}
		}
	}
}



process Jenna::tendBar() 
{
	[[PT_ACTIVITY, referent]];

	startup 
	{
		pathEgg:Item;
		counter,counter2:Number;
	}

	process
	{
		for counter = 10 to 17 {

			foreach pathEgg where (type = TypePathMarker)and (frame = counter) within 30 cells of Jenna
			{
				if pathfind(pathEgg.referent,30)
				{
					if (counter = 11) or (counter = 15)
					{
						if counter = 11 
						{
							closeEWDoor();
						}
		
						else
						{
							closeNSDoor();
						}
					}
					else
					{
						wait(100);
						randomBarkAtBar();
					}
				}
				else
				{
					wait(500);
				}
			}
			foreach pathEgg where (type = TypeBarCounter) within 5 cells of Jenna
			{
				pathfind(pathEgg.referent,5);
				break;
			}
			foreach pathEgg where (type = TypeBarCounter) within 2 cells of Jenna
			{
				turnToFace(getDirToItem(pathEgg.referent),false,10);
				realDoAnim(AS_WALK,getDir());
				realDoAnim(AS_CLIMB_4,getDir());
				break;
			}
		}
	}
}


process Jenna::randomBarkAtBar()
{
	[[PT_ACTIVITY, referent]];

	avaP,torP : WorldPoint;
	ran:Number;

	if somebodyTalking
	{
		wait(100);
		realDoAnim(AS_FIDGET,getDir());
		return;
	}


	ran = getDistance(Avatar:Referent, Jenna:Referent);

	if ran < 200 
	{
		ran = urandom(8);									  

  
		if ran = 1 
		{
			turnToFace(getDirToItem(Avatar:Referent),false,10);
			bark("Excuse me, I'm trying to work.
				\f Excuse-moy, mais j'essaie de travailler, moy.
				\g Verzeihung, aber ich versuche, zu arbeiten.");
			realDoAnim(AS_FIDGET,getDir());
			wait(120);
		}
		if ran = 2 
		{
			turnToFace(getDirToItem(Avatar:Referent),false,10);
			if not orlokMet 
			{
				bark("Need something? \f T'as besoin de quelque chose?
				\g Braucht Ihr etwas?");
				return;
			}

			else
			{
				#if English
				{
				bark("Need something?");
				}
				#if French
