process Jenna::closeEWDoor()
{
	[[PT_ACTIVITY, referent]];

		sped : Item;
		door : Door;

		foreach	sped where (type = TypeDoorEW) within 5 cells of Jenna {
			door.referent = sped.referent;
			break;
		}

		if door 
		{
			turnToFace(getDirToItem(door.referent),false,10);
			bark("Why is this open?
				\f Pourquoi c'est ouvert, áa?
				\g Weshalb steht die TÅr offen?");
			door.doorDriver(Jenna:Referent);
		}
}


